{
  "name": "undici-fetch",
  "version": "1.0.0-rc.3",
  "description": "A WHATWG Fetch implementation based on @nodejs/undici",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "lint:js": "standard --verbose",
    "lint:ts": "ts-standard --verbose",
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "test": "tap test/*.js --test-ignore='testUtils.js --no-coverage",
    "test:tdd": "tap test/*.js -w --test-ignore='testUtils.js  --no-coverage-report",
    "test:typescript": "tsd",
    "coverage": "standard | snazzy && tap test/*.js",
    "coverage:ci": "npm run coverage -- --coverage-report=lcovonly",
    "bench": "node benchmarks/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ethan-Arrowood/undici-fetch.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "Ethan Arrowood <ethan@arrowood.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ethan-Arrowood/undici-fetch/issues"
  },
  "homepage": "https://github.com/Ethan-Arrowood/undici-fetch#readme",
  "dependencies": {
    "undici": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "abortcontroller-polyfill": "^1.7.1",
    "axios": "^0.21.1",
    "minipass-fetch": "^1.3.2",
    "node-fetch": "^2.6.1",
    "semver": "^7.3.5",
    "standard": "^16.0.3",
    "snazzy": "^9.0.0",
    "tap": "^15.0.6",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "tsd": {
    "directory": "test/types",
    "compilerOptions": {
      "esModuleInterop": true
    }
  }
}
